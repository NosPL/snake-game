<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<HBox prefHeight="135.0" prefWidth="268.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.noscompany.snakejavafxclient.game.ButtonsController">
   <children>
      <VBox prefHeight="37.0" prefWidth="130.0">
         <children>
            <AnchorPane prefHeight="96.0" prefWidth="130.0">
               <children>
                  <Button fx:id="startButton" mnemonicParsing="false" onAction="#startGame" prefHeight="52.0" prefWidth="130.0" text="START" />
                  <Button fx:id="cancelButton" layoutY="72.0" mnemonicParsing="false" onAction="#cancelGame" prefHeight="52.0" prefWidth="130.0" text="CANCEL" />
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="10.0" top="20.0" />
         </padding>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Button fx:id="pauseButton" mnemonicParsing="false" onAction="#pauseGame" prefHeight="52.0" prefWidth="130.0" text="PAUSE" />
                  <Button fx:id="resumeButton" layoutY="72.0" mnemonicParsing="false" onAction="#resumeGame" prefHeight="52.0" prefWidth="130.0" text="RESUME" />
               </children>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="20.0" left="10.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</HBox>
