<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="507.0" prefWidth="319.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.noscompany.snakejavafxclient.game.online.ChatController">
   <children>
      <AnchorPane prefHeight="238.0" prefWidth="245.0">
         <children>
            <VBox prefHeight="462.0" prefWidth="279.0">
               <children>
                  <TableView fx:id="chatTableView" prefHeight="515.0" prefWidth="279.0">
                    <columns>
                      <TableColumn prefWidth="264.0" text="CHAT" />
                    </columns>
                     <padding>
                        <Insets bottom="30.0" left="10.0" right="30.0" top="10.0" />
                     </padding>
                  </TableView>
                  <TextField fx:id="chatMessageTextField">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </TextField>
                  <Button fx:id="sendChatMessageButton" onAction="#sendMessage" mnemonicParsing="false" prefHeight="28.0" prefWidth="331.0" text="SEND">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
